# Bolt AI Environment Configuration
# Copy this file to .env.local and add your API keys

# Application Settings
NODE_ENV=development
NEXT_PUBLIC_VERSION=0.0.7

# AI Provider API Keys
ANTHROPIC_API_KEY=your-anthropic-api-key-here
OPENAI_API_KEY=your-openai-api-key-here
GROQ_API_KEY=your-groq-api-key-here
HUGGINGFACE_API_KEY=your-huggingface-api-key-here
OPEN_ROUTER_API_KEY=your-openrouter-api-key-here
TOGETHER_API_KEY=your-together-api-key-here
DEEPSEEK_API_KEY=your-deepseek-api-key-here
GOOGLE_GENERATIVE_AI_API_KEY=your-google-generative-ai-api-key-here
MISTRAL_API_KEY=your-mistral-api-key-here
XAI_API_KEY=your-xai-api-key-here
PERPLEXITY_API_KEY=your-perplexity-api-key-here

# Local Provider Base URLs
OLLAMA_API_BASE_URL=http://localhost:11434
LMSTUDIO_API_BASE_URL=http://localhost:1234
OPENAI_LIKE_API_BASE_URL=http://localhost:8000
TOGETHER_API_BASE_URL=https://api.together.xyz

# AWS Bedrock Configuration (JSON string)
AWS_BEDROCK_CONFIG={"region":"us-east-1","accessKeyId":"your-access-key","secretAccessKey":"your-secret-key"}

# GitHub Integration
GITHUB_ACCESS_TOKEN=your-github-access-token-here

# Netlify Integration
NETLIFY_TOKEN=your-netlify-token-here

# Docker Environment
RUNNING_IN_DOCKER=false

# Logging
VITE_LOG_LEVEL=info

# Development Settings
DEFAULT_NUM_CTX=4096
